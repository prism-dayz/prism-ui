{
  "name": "archaeon",
  "version": "0.5.0",
  "description": "nodejs express postgress aws",
  "main": "src/index.js",
  "repository": "https://github.com/brngdsn/archaeon",
  "author": "brngdsn",
  "license": "MIT",
  "scripts": {
    "initdb:osx": "cat ./sql/init.sql | sudo -u methanogen psql -d archaeon",
    "createdb:osx": "createdb archaeon",
    "ec2": "ssh -i ./archaeon.pem ec2-user@$EC2_HOST",
    "initdb": "cat ./sql/init.sql | sudo -u postgres psql -d archaeon",
    "createdb": "cat ./sql/create-db.sql | sudo -u postgres psql",
    "dev:api": "nodemon --watch src src/index.js",
    "serve:api": "node src/index.js",
    "dev:spa": "npm run serve:spa",
    "serve:spa": "vue-cli-service serve",
    "build:spa": "vue-cli-service build",
    "startdb:osx": "pg_ctl -D /usr/local/var/postgres start"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "buefy": "^0.8.18",
    "buffer": "^5.6.0",
    "canvas": "^2.6.1",
    "connect-pg-simple": "^6.1.0",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.6.4",
    "cors": "^2.8.5",
    "directory-tree": "^2.2.4",
    "discord.js": "^12.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "html2canvas": "^1.0.0-rc.5",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.13",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "passport-remember-me": "^0.0.1",
    "pg": "^8.1.0",
    "promise.waterfall": "^3.2.0",
    "quasar": "^1.12.13",
    "request": "^2.88.2",
    "vue": "^2.6.11",
    "vue-fontawesome": "^0.0.2",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.1.6",
    "vue-window-size": "^0.2.5",
    "vuex": "^3.1.3",
    "ws": "^7.2.5",
    "xml-formatter": "^2.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.3.0",
    "@vue/cli-service": "^4.3.0",
    "vue-template-compiler": "^2.6.11"
  }
}
